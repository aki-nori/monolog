<p>Item/index</p>
<div class="row">
  <% @items.each do |item| %>
  <div class="col-md-3">
    <table>
      <tr>
        <td>
          <%= attachment_image_tag item, :image, :fill, 100, 100, fallback: "no_image.jpg" %>
        </td>
      </tr>
      <tr>
        <td>
          <p>
            <%= link_to item.name, item_path(item) %>
          </p>
        </td>
      </tr>
      <tr>
        <td>
          <%= render 'likes/like_form', item: item %>
        </td>
      </tr>
    </table>
  </div>
  <% end %>
</div>